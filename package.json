{
  "name": "minimal-11ty-setup",
  "scripts": {
    "dev": "npm-run-all --parallel dev:runDevTasks dev:runEleventy",
    "dev:runDevTasks": "gulp devTasks",
    "dev:runEleventy": "npx @11ty/eleventy --quiet --serve",
    "build": "npm-run-all build:runBuildTasks build:runEleventy",
    "build:runBuildTasks": "gulp buildTasks",
    "build:runEleventy": "npx @11ty/eleventy"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@semantic-release/git": "^10.0.1",
    "alpinejs": "^3.10.3",
    "autoprefixer": "^10.4.8",
    "cssnano": "^5.1.12",
    "del": "^6.1.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "rollup": "^2.77.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^19.0.3",
    "tailwindcss": "^3.2.3"
  }
}
